{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-builder --ia32"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@sentry/cli": "^1.58.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "electron-download": "^4.1.1"
  },
  "dependencies": {
    "@sentry/electron": "^2.0.1"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://www.example.com"
      }
    ],
    "appId": "helloworld.com",
    "productName": "Test",
    "extraFiles": [],
    "extraResources": [],
    "win": {
      "target": [
        "portable"
      ],
      "publisherName": "Test company",
      "icon": "favicon.ico"
    },
    "msi": {
      "perMachine": true
    }
  }
}
